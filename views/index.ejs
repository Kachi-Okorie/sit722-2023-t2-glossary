<!DOCTYPE html>
<html>

<head>
  <title>
    <%= title %>
  </title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
 
</head>

<body>
  <h1>
    <%= title %>
  </h1>
  <table>
    <tr>
      <th>ID#</th>
      <th>Term</th>
      <th style="width:50%">Description</th>
      <th>References</th>
    </tr>
    <% names.forEach((name) => { %>
      <tr>
        <td>
          <%= name.id %>
        </td>
        <td>
          <%= name.Term %>
        </td>
        <td>
          <%= name.Description %>
        </td>
        <td>
          <%= name.References %>
        </td>
      </tr>
      <% }) %>
  </table>
 

</body>

</html>